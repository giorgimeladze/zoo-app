<table>
  <thead>
    <tr>
      <th score="col">Specie</th>
      <th score="col">Age</th>
      <th score="col">Color</th>
      <th score="col">Sex</th>
      <th score="col">Quantity</th>
    </tr>
  </thead>
  <tbody>
    <% @animals.each do |animal| %>
      <tr>
        <td><%= animal.specie %></td>
        <td><%= animal.age %></td>
        <td><%= animal.color %></td>
        <td><%= animal.sex %></td>
        <td><%= animal.quantity %></td>
      </tr>
    <% end %>
  </tbody>
</table>
